{
  "name": "madgrades",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "ajv": "^8.17.1",
    "axios": "^1.12.0",
    "classnames": "^2.2.5",
    "dom-to-image": "^2.6.0",
    "dotenv": "^16.4.7",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.5",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.1.16",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^3.1.0",
    "sass": "^1.80.0",
    "semantic-ui-css": "^2.3.0",
    "semantic-ui-react": "^3.0.0-beta.2",
    "universal-cookie": "^7.2.1"
  },
  "scripts": {
    "git-info": "git log -1 --oneline > src/assets/git-rev.txt",
    "build-css": "sass -I ./src -I ./node_modules src/:src/ --no-source-map",
    "watch-css": "npm run build-css && sass -I src/ ./node_modules src/:src/ --watch",
    "start-js": "vite",
    "start": "npm-run-all -p git-info watch-css start-js",
    "build-js": "vite build",
    "build": "npm-run-all git-info build-css build-js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy": "npm run build && mv build/index.html build/200.html && surge build",
    "deploy-staging": "npm run build && mv build/index.html build/200.html && surge build staging.madgrades.com",
    "deploy-prod": "npm run build && mv build/index.html build/200.html && surge build madgrades.com"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@types/dom-to-image": "^2.6.7",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.2.3",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vitejs/plugin-react": "^5.1.3",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "surge": "^0.24.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-sass-dts": "^1.3.35"
  },
  "overrides": {
    "surge": {
      "tough-cookie": "^4.1.3",
      "form-data": "^4.0.0",
      "axios": "^1.7.8",
      "request": "^2.88.2"
    },
    "semantic-ui-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
